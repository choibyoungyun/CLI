            "line2"    : {"format" : "\tNODE_NO: {0}\n", "value" : "response.body.NODE_NO"},
            "comment2" : "\t--------------------------------------------------------------------\n",
            "line3"    : {
                          "format" : "\t{0:>8s} {1:>10s} {2:>10s} {3:>10s} {4:>10s} {5:>8s} {6:>8s}\n",
                          "value"  : "display.field.COL0,
